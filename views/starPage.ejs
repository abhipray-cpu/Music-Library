<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
        <% title %>
    </title>
    <link rel="stylesheet" href="/css/bio.css">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.15.4/css/fontawesome.min.css" integrity="sha384-jLKHWM3JRmfMU0A5x5AkjWkw/EYfGUAGagvnfryNV3F9VqM98XiIH7VBGVoxVSc7" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css" integrity="sha512-YWzhKL2whUzgiheMoBFwW8CKV4qpHQAEuvilg9FAn5VJUDwKZZxkJNuGM4XkWuk94WCrrwslk8yWNGmY1EduTA==" crossorigin="anonymous" referrerpolicy="no-referrer"
    />

</head>

<body>
    <div id="container">
        <!--this will contain the profile-->
        <div id="row1" style=" background-image: url('/<%= image %>');">
            <div id="name">
                <h1>
                    <%= title %>
                </h1>
            </div>
            <div id="info">
                <div class="details">
                    <h2>Songs:
                        <%= songs.length%>
                    </h2>
                </div>
                <div class="details">
                    <h2>Plays:
                        <%= plays%>
                    </h2>
                </div>
                <div class="details">
                    <h2>Likes:
                        <%= likes %>
                    </h2>
                </div>
            </div>
            <div id="top-songs">
                <!--top 5 songs-->
                <% if(topSongs.length>0) {%>
                    <% topSongs.forEach(song =>{  %>
                        <div class="song">
                            <div class="topImage">
                                <img src="/<%= song.coverPic%>" alt="<%= song.title %>">
                            </div>
                            <div class="topTitle">
                                <%= song.title %>
                            </div>
                        </div>
                        <% }) %>
                            <% } else {%>
                                <div class="song">
                                    <div class="topImage">
                                        <img src="https://www.pinkvilla.com/imageresize/ice-cream-ariana-grande-calls-selena-gomez-ice-queen-after-the-latters-collab-with-blackpink-drops_0.jpg?width=752&format=webp&t=pvorg" alt="">
                                    </div>
                                    <div class="topTitle">
                                        Song1
                                    </div>
                                </div>
                                <div class="song">
                                    <div class="topImage">
                                        <img src="https://i.ytimg.com/vi/BzE1mX4Px0I/maxresdefault.jpg" alt="">
                                    </div>
                                    <div class="topTitle">
                                        Song1
                                    </div>
                                </div>
                                <div class="song">
                                    <div class="topImage">
                                        <img src="https://i.ytimg.com/vi/D5drYkLiLI8/maxresdefault.jpg" alt="">
                                    </div>
                                    <div class="topTitle">
                                        Song1
                                    </div>
                                </div>
                                <div class="song">
                                    <div class="topImage">
                                        <img src="data:image/jpeg;base64,"
                                            alt="">
                                    </div>
                                    <div class="topTitle">
                                        Song1
                                    </div>
                                </div>
                                <div class="song">
                                    <div class="topImage">
                                        <img src="https://i1.sndcdn.com/artworks-m59ZJDa9EItOzvgc-pZ4apw-t500x500.jpg" alt="">
                                    </div>
                                    <div class="topTitle">
                                        Song1
                                    </div>
                                </div>
                                <% } %>
            </div>
        </div>
        <div id="row2">
            <div class="heading">
                <div class="Hcol" style="margin-left:8%">Playing</div>
                <div class="Hcol">Cover</div>
                <div class="Hcol">Song</div>
                <div class="Hcol">Plays</div>
                <div class="Hcol">Likes</div>
                <div class="Hcol">Duration</div>
                <div class="Hcol">Like</div>

            </div>
            <div id="gaane">
                <% if (songs.length > 0) {%>
                    <% songs.forEach(song =>{  %>
                        <div class="audio">
                            <div class="col1">
                                <i class="fa-solid fa-play"></i>
                            </div>
                            <div class="image">
                                <img src="/<%=song.coverPic %>" alt="<%= song.title%>">
                            </div>
                            <div class="title">
                                <%= song.title%>
                            </div>
                            <div class="plays">
                                <%=song.plays %>Plays</div>
                            <div class="likes">
                                <%= song.likes %> Likes</div>
                            <div class="duration">
                                <%= song.duration %>
                            </div>
                            <div class="like">
                                <i class="fa-solid fa-heart liked"></i>
                            </div>

                        </div>
                        <% }) %>
                            <% } else { %>
                                <!--this is the state that will be used for playing song-->
                                <div class="playing">
                                    <div class="wrapperL">
                                        <div class="line1L barL">
                                        </div>
                                        <div class="line2L barL">
                                        </div>
                                        <div class="line3L barL">
                                        </div>
                                        <div class="line4L barL">
                                        </div>
                                        <div class="line5L barL">
                                        </div>
                                        <div class="line6L barL">
                                        </div>
                                        <div class="line7L barL">
                                        </div>
                                    </div>
                                    <div class="image">
                                        <img src="https://www.pinkvilla.com/imageresize/ice-cream-ariana-grande-calls-selena-gomez-ice-queen-after-the-latters-collab-with-blackpink-drops_0.jpg?width=752&format=webp&t=pvorg" alt="Song1">
                                    </div>
                                    <div class="title">Song1</div>
                                    <div class="plays">1000 Plays</div>
                                    <div class="likes">100 Likes</div>
                                    <div class="duration">3:40</div>
                                    <div class="like">
                                        <i class="fa-solid fa-heart liked"></i>
                                    </div>

                                </div>

                                <div class="audio">
                                    <div class="col1">
                                        <i class="fa-solid fa-play"></i>
                                    </div>
                                    <div class="image">
                                        <img src="https://www.pinkvilla.com/imageresize/ice-cream-ariana-grande-calls-selena-gomez-ice-queen-after-the-latters-collab-with-blackpink-drops_0.jpg?width=752&format=webp&t=pvorg" alt="Song1">
                                    </div>
                                    <div class="title">Song1</div>
                                    <div class="plays">1000 Plays</div>
                                    <div class="likes">100 Likes</div>
                                    <div class="duration">3:40</div>
                                    <div class="like">
                                        <i class="fa-solid fa-heart liked"></i>
                                    </div>

                                </div>
                                <div class="audio">
                                    <div class="col1">
                                        <i class="fa-solid fa-play"></i>
                                    </div>
                                    <div class="image">
                                        <img src="https://www.pinkvilla.com/imageresize/ice-cream-ariana-grande-calls-selena-gomez-ice-queen-after-the-latters-collab-with-blackpink-drops_0.jpg?width=752&format=webp&t=pvorg" alt="Song1">
                                    </div>
                                    <div class="title">Song1</div>
                                    <div class="plays">1000 Plays</div>
                                    <div class="likes">100 Likes</div>
                                    <div class="duration">3:40</div>
                                    <div class="like">
                                        <i class="fa-solid fa-heart liked"></i>
                                    </div>

                                </div>
                                <div class="audio">
                                    <div class="col1">
                                        <i class="fa-solid fa-play"></i>
                                    </div>
                                    <div class="image">
                                        <img src="https://www.pinkvilla.com/imageresize/ice-cream-ariana-grande-calls-selena-gomez-ice-queen-after-the-latters-collab-with-blackpink-drops_0.jpg?width=752&format=webp&t=pvorg" alt="Song1">
                                    </div>
                                    <div class="title">Song1</div>
                                    <div class="plays">1000 Plays</div>
                                    <div class="likes">100 Likes</div>
                                    <div class="duration">3:40</div>
                                    <div class="like">
                                        <i class="fa-solid fa-heart liked"></i>
                                    </div>

                                </div>
                                <% } %>
            </div>




        </div>
        <div id="row3">
            <%- include('include/playerStar.ejs') %>
        </div>
    </div>
</body>

</html>